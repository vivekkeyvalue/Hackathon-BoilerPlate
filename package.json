    "lint-staged": "^10.5.4",
    "postcss": "^8.4.14",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "stylelint-config-recommended": "^7.0.0",
    "tailwindcss": "^3.1.5",
    "webpack": "5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,scss,md}": [
      "eslint",
      "prettier"
    ],
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
